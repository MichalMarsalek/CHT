{
  "name": "cht",
  "scopeName": "source.cht",
  "patterns": [
    {
      "begin": "(\"(?:\\\\.|[^\"])*\"|(?<![\\w:])\\d+:\\d+(?![\\w:])|[^\\s\"():#]+)\\s*(?=[:(])",
      "beginCaptures": {
        "1": { "name": "entity.name.function.cht" }
      },
      "end": "(?=[:(])",
      "patterns": []
    },
    { "include": "#comment" },
    { "include": "#quoted" },
    { "include": "#raw" },
    { "include": "#parens" },
    { "include": "#colon" }
  ],
  "repository": {
    "comment": {
      "patterns": [
        {
          "name": "comment.line.number-sign.cht",
          "match": "# .*$"
        }
      ]
    },
    "quoted": {
      "patterns": [
        {
          "name": "string.quoted.double.cht",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.cht",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "raw": {
      "patterns": [
        {
          "name": "string.unquoted.raw.cht",
          "match": "(?<![\\w:])\\d+:\\d+(?![\\w:])|[^\\s\"():]+"
        }
      ]
    },
    "parens": {
      "patterns": [
        {
          "name": "punctuation.group.parens.begin.cht",
          "match": "\\("
        },
        {
          "name": "punctuation.group.parens.end.cht",
          "match": "\\)"
        }
      ]
    },
    "colon": {
      "patterns": [
        {
          "name": "punctuation.separator.children-colon.cht",
          "match": ":"
        }
      ]
    }
  }
}
